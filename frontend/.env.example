# Frontend Environment Variables Template
# Copy this file to .env and modify as needed

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/chat

# Voice Features
REACT_APP_VOICE_ENABLED=true
REACT_APP_DEFAULT_LANGUAGE=vi-VN

